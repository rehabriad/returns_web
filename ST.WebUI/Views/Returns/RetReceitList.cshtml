@model IEnumerable<ST.Entity.RetReceit>

@{
    var i = 0;
}


<table id="retReceitTbl" class="table table-striped table-bordered table-hover table-responsive" >
    <tr>
        <th>@Html.DisplayNameFor(model => model.recietcode)</th>
        <th>
            @Html.DisplayNameFor(model => model.recietno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.recietserialfrom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.recietserialto)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        ViewBag.Rowcount = i;
        @Html.Partial("_RetReceitSingle", item)
        i++;
    }
</table>

<table>
    <tr>
        <td>
            <a id="retReceitAdd" data-request-url="@Url.Action("AddRetReceit", "returns")" class="btn btn-default">@ST.Resource.ReturnsResource.viewRetReceitListAddReceit</a>
        </td>
    </tr>
</table>
